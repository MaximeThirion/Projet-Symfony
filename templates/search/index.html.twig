{% extends 'base.html.twig' %}

{% block title %}{{ title }}{% endblock %}

{% block body %}
<div class="container">
    <h1>Search</h1>
    <table class="table">
        <thead>
        <tr>
            <th scope="col">Type</th>
            <th scope="col">Label</th>
        </tr>
        </thead>
        <tbody>
        {% for donnee in donnees %}
            <tr>
                <td>{{ donnee.type }}</td>
                <td>{{ donnee.label }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
{% endblock %}
